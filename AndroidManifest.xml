<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.project2013"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:hasCode="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:requiredForAllUsers="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.project2013.MainActivity"
            android:label="@string/name_shown" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name="com.example.project2013.ContactContentActivity" 
            android:label="@string/name_shown" />
        <activity
            android:name="com.example.project2013.NewContentActivity"
            android:label="@string/name_shown" >
        </activity>
        <activity
            android:name="com.example.project2013.AgendaContentActivity"
            android:label="@string/name_shown" >
        </activity>
    </application>

</manifest>